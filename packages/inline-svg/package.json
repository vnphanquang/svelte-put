{
	"name": "@svelte-put/inline-svg",
	"version": "4.0.0",
	"description": "solution to inline SVGs in svelte land",
	"main": "src/index.js",
	"module": "src/index.js",
	"types": "types/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./types/index.d.ts",
			"import": "./src/index.js"
		},
		"./preprocessor": {
			"types": "./types/index.d.ts",
			"import": "./src/preprocessor/index.js"
		},
		"./vite": {
			"types": "./types/index.d.ts",
			"import": "./src/vite/index.js"
		}
	},
	"typesVersions": {
		"*": {
			"preprocessor": [
				"./types/index.d.ts"
			],
			"vite": [
				"./types/index.d.ts"
			]
		}
	},
	"publishConfig": {
		"access": "public",
		"provenance": true
	},
	"files": [
		"src",
		"types"
	],
	"scripts": {
		"lint": "eslint .",
		"format": "prettier --ignore-path ../../.prettierignore --write .",
		"dts": "dts-buddy --write && publint",
		"prepublishOnly": "turbo run dts --filter=@svelte-put/inline-svg"
	},
	"keywords": [
		"svelte",
		"action",
		"svg",
		"inline",
		"dynamic",
		"static",
		"build",
		"runtime",
		"vite",
		"preprocessor",
		"static",
		"compiletime"
	],
	"author": {
		"email": "vnphanquang@gmail.com",
		"name": "Quang Phan",
		"url": "https://github.com/vnphanquang"
	},
	"license": "MIT",
	"homepage": "https://github.com/vnphanquang/svelte-put/tree/main/packages/inline-svg",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vnphanquang/svelte-put.git",
		"directory": "packages/inline-svg"
	},
	"bugs": {
		"url": "https://github.com/vnphanquang/svelte-put/issues"
	},
	"devDependencies": {
		"@internals/tsconfig": "workspace:*",
		"@types/lodash.debounce": "^4.0.9",
		"@types/svg-parser": "^2.0.6"
	},
	"dependencies": {
		"hast-util-to-html": "^9.0.3",
		"lodash.debounce": "^4.0.8",
		"magic-string": "^0.30.14",
		"svelte-parse-markup": "^0.1.5",
		"svg-parser": "^2.0.4",
		"zimmerframe": "^1.1.2"
	},
	"peerDependencies": {
		"svelte": "^5.1.0",
		"vite": "^5.4.8 || ^6.0.0"
	},
	"volta": {
		"extends": "../../package.json"
	}
}
