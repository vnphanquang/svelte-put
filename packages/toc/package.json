{
	"name": "@svelte-put/toc",
	"version": "6.0.0-next.0",
	"description": "Action & utilities for building table of contents",
	"main": "src/index.js",
	"module": "src/index.js",
	"types": "types/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./types/index.d.ts",
			"import": "./src/index.js"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"src",
		"types"
	],
	"scripts": {
		"lint": "eslint . --ignore-path=\"../../.eslintignore\"",
		"format": "prettier --ignore-path ../../.prettierignore --write .",
		"dts": "dts-buddy --write && publint",
		"prepublishOnly": "turbo run dts --filter=@svelte-put/toc"
	},
	"keywords": [
		"svelte",
		"action",
		"tableofcontents",
		"toc",
		"@svelte-put",
		"svelte-put",
		"table-of-contents",
		"table of contents",
		"table",
		"content",
		"utility",
		"store",
		"intersection observer",
		"intersection",
		"observer",
		"anchor"
	],
	"author": {
		"email": "vnphanquang@gmail.com",
		"name": "Quang Phan",
		"url": "https://github.com/vnphanquang"
	},
	"license": "MIT",
	"homepage": "https://github.com/vnphanquang/svelte-put/tree/main/packages/toc",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vnphanquang/svelte-put.git",
		"directory": "packages/toc"
	},
	"bugs": {
		"url": "https://github.com/vnphanquang/svelte-put/issues"
	},
	"devDependencies": {
		"@internals/tsconfig": "workspace:*"
	},
	"peerDependencies": {
		"svelte": "^5.0.0-next.152"
	},
	"volta": {
		"extends": "../../package.json"
	}
}
